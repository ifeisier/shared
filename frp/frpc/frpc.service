[Unit]
Description = frp client
After = network.target syslog.target
Wants = network.target

[Service]
Type = simple
ExecStart = /root/frp_0.63.0_linux_amd64/frpc -c /root/frp_0.63.0_linux_amd64/frpc.toml
Restart=always
RestartSec=5

[Install]
WantedBy = multi-user.target

